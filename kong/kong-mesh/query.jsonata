{
  "releases": $sort(
    $.{
      "changelogUrl": "https://docs.konghq.com/mesh/changelog" & "#" & $replace(version, /\./, ""),
      "digest": checksum_sha256,
      "isStable": true,
      "newDigest": checksum_sha256,
      "releaseTimestamp": uploaded_at,
      "sourceUrl": cdn_url,
      "version": version
    },
    function($l, $r) {
      $l.version < $r.version
    }
  ),
  "homepage": "https://konghq.com/products/kong-mesh"
}
